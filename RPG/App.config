                <TextBox id="12" speaker="Nick" speakerPicture="\\Faces\\Nick\\Nick_Happy_Face" listener="Elena" listenerPicture="\\Faces\\Elena\\ Elena_NotAmused_Face" row1="Oh Ja!" row2="Ich bin Nick Walt, Sklavenhändler." row3="" row4=""/>
