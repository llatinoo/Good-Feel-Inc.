                <TextBox id="7" speaker="Jos" speakerPicture="\\Faces\\Jos\\Jos_Happy_Face" listener="Elena" listenerPicture="\\Faces\\Elena\\ Elena_Shocked_Face" row1="Ja der alte Bastard ist mein Vater." row2="Aber wir haben uns nie verstanden." row3="Hat mich am Ende in " row4="die Armee gezwungen."/>
                <TextBox id="8" speaker="Elena" speakerPicture="\\Faces\\Elena\\ Elena_Shocked_Face" listener="Jos" listenerPicture="\\Faces\\Jos\\Jos_ Happy _Face" row1="..." row2="Ich dachte du hast was drauf." row3="" row4=""/>
                <TextBox id="9" speaker="Jos" speakerPicture="\\Faces\\Jos\\Jos_Happy_Face" listener="Elena" listenerPicture="\\Faces\\Elena\\ Elena_Shocked_Face" row1="Ach komm, in Winterbach" row2="zeig ich dir schon was ich so drauf hab!" row3="" row4=""/>
